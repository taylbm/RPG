		$(".toggle").on('slidestop',function(){		
			var controlname = $(this).attr("id")	
			var cancel = $(this).val();	
			console.log(cancel)
			var present;var previous;
			if (cancel=="on"){present = "on";previous = "off"}else{present="off";previous="on"}


			if (confirm('Are you sure you want to change '+controlname+'?')){

				$(this).val(present)
			
			
			}
			else{$(this).val(previous)}
			$(this).slider('refresh')
			if ($(this).val()=="on"){$(this).slider('disable');$("#"+controlname).css('background-color','#51FF22');}

		});




			

	});
